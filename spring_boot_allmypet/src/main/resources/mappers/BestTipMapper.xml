<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper
	namespace="com.spring_boot_allmypet.project.dao.board.IBestTipDAO">
	<select id="getTipList" parameterType="String"
		resultType="com.spring_boot_allmypet.project.model.board.BestTipVO">
		SELECT *
		FROM mytip_board
		WHERE petCtgNo = #{petCtgNo}
		ORDER BY postLike DESC
	</select>
</mapper>